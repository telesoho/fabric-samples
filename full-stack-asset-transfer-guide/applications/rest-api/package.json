{
  "name": "restapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start-restapi",
    "prod": "npm run build && npm run start",
    "test": "jest",
    "pm2": "pm2",
    "pm2:prod": "pm2 start restapi.config.js --env production",
    "pm2:dev": "pm2 start restapi.config.js --env development",
    "start-restapi": "node --require source-map-support/register ./dist/index.js",
    "start:dev": "node --require source-map-support/register ./dist/index.ts| pino-pretty",
    "watch:debug": "NODE_ENV=development nodemon --inspect=5858 -e ts --exec node -r ts-node/register --require source-map-support/register ./src/index.ts | pino-pretty",
    "convert-swagger": "node scripts/convert-swagger.js",
    "prestart": "npm run convert-swagger",
    "prebuild": "npm run convert-swagger",
    "postbuild": "cp ./src/*_swagger.json ./dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.18.6",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/pino": "^6.3.12",
    "@types/pino-http": "^5.8.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "pg-mem": "^3.0.4",
    "pino-pretty": "^5.1.3",
    "pm2": "^5.4.3",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.4",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.2",
    "@hyperledger/fabric-gateway": "^1.7.1",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "env-var": "^7.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "fabric-ca-client": "^2.2.20",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "js-yaml": "^4.1.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "pg-boss": "^10.1.6",
    "pg-promise": "^11.10.2",
    "pino": "^6.14.0",
    "pino-http": "^5.8.0",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  }
}
